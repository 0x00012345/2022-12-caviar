{
  "name": "caviar",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:outdoteth/Caviar.git",
  "author": "outdoteth <outdoteth@users.noreply.github.com>",
  "license": "MIT",
  "scripts":{
    "size": "hardhat size-contracts",
    "build": "hardhat compile",
    "deploy": "hardhat deploy:all",
    "typechain": "hardhat typechain",
    "lint": "tsc --noEmit -skipLibCheck && solhint 'contracts/**/*.sol' && prettier --check 'contracts/**/*.sol' 'deploy/**/*.ts' 'test/**/*.ts' && eslint . --ext .ts",
    "fork": "hardhat node --verbose",
    "format": "prettier --write 'contracts/**/*.sol' 'deploy/**/*.ts' 'test/**/*.ts'",
    "clean": "hardhat clean",
    "transfer:tokens": "npx hardhat run ./deploy/tasks/deployments/dev/2.transfer-tokens.ts --network localhost"
  },
  "dependencies": {
    "ethers": "^5.7.2",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.3.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "2.0.3",
    "@tenderly/hardhat-tenderly": "1.1.0-beta.5",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/lowdb": "1.0.9",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "eth-sig-util": "2.5.3",
    "ethereum-waffle": "3.2.0",
    "ethereumjs-util": "7.0.2",
    "hardhat": "^2.12.0",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.11.4",
    "hardhat-gas-reporter": "^1.0.0",
    "hardhat-typechain": "^0.3.3",
    "lowdb": "1.0.0",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "4.7.4"
  }
}
